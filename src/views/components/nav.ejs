<script>
const renderMenu = () => {
  const openB = document.getElementById('open-menu')

  const closeB = document.getElementById('close-menu')
  const getClass = document.querySelector('#menu.xl\\:translate-x-0')

  openB.addEventListener('click', () => {
    if (getClass.classList.contains('-translate-x-80')) {
      getClass.classList.remove('-translate-x-80')
    } else {
      getClass.classList.add('-translate-x-80')
    }
  })
  closeB.addEventListener('click', () => {
    getClass.classList.add('-translate-x-80')
  })
}
document.addEventListener('DOMContentLoaded', function () {
  renderMenu()
})
</script>
