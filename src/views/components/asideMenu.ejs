<script>
	const btn = document.getElementById('logout-btn')	
	btn.onclick = (async (ev) => {
		ev.preventDefault()
		console.log('Cliecked')
		const logoutFetch = await fetch('/logout', {
			method:"POST"
		})
		if(logoutFetch.status === 200) window.location.reload()
	})

</script>
